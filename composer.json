{
  "name": "mvo/contao-facebook",
  "type": "contao-bundle",
  "description": "Facebook Integration for Contao",
  "keywords": [
    "contao",
    "facebook",
    "social",
    "posts",
    "events",
    "opengraph"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Moritz Vondano",
      "homepage": "https://github.com/m-vo/contao-facebook"
    }
  ],
  "require": {
    "php": ">=7.1",
    "contao/core-bundle": "^4.4.0",
    "facebook/graph-sdk": "^5.5",
    "guzzlehttp/guzzle": "~6.0",
    "codefog/contao-haste": "^4.17.2"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Mvo\\ContaoFacebook\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Mvo\\ContaoFacebook\\ContaoManagerPlugin"
  }
